@startuml
!include ../style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":u:CloseClaimCommand" as CloseClaimCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box
[-> LogicManager : execute(CloseClaimCommand)
activate LogicManager

LogicManager -> AddressBookParser : parseCommand(input string)
activate AddressBookParser

create CloseClaimCommand
AddressBookParser -> CloseClaimCommand
activate CloseClaimCommand

CloseClaimCommand --> AddressBookParser
deactivate CloseClaimCommand

AddressBookParser --> LogicManager : u
deactivate AddressBookParser

LogicManager -> CloseClaimCommand : execute()
activate CloseClaimCommand

CloseClaimCommand -> Model : closeClaim(claim inputs)
activate Model

Model --> CloseClaimCommand : Claim
deactivate Model

CloseClaimCommand --> LogicManager
deactivate CloseClaimCommand
CloseClaimCommand -[hidden]-> LogicManager : result
destroy CloseClaimCommand

[<--LogicManager
deactivate LogicManager
@enduml
